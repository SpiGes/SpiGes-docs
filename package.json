{
  "name": "spiges-handbook",
  "version": "0.0.1",
  "description": "spiges-handbook web site",
  "repository": "github:spiges/handbook",
  "homepage": "https://spiges.github.io/handbook",
  "author": "SpiGes team",
  "license": "CC BY-NC-ND 4.0",
  "spelling": "cSpell:ignore HTMLTEST precheck postbuild -",
  "scripts": {
    "_prod-env": "npx cross-env NODE_ENV=production HUGO_ENV=production HUGO_ENVIRONMENT=production",
    "_dev-env": "npx cross-env NODE_ENV=development",
    "_hugo": "hugo --logLevel debug --gc --cleanDestinationDir",
    "_build": "npm run clean && npm run _hugo -- --minify",
    "_build:dev": "npm run _dev-env && npm run _build -- -e dev -DFE",
    "_build:prod": "npm run _prod-env && npm run _build",
    "_build:ci:prod": "npm run _prod-env && npm run _build -- --baseURL \"${BASE_URL}\"",
    "_serve:dev": "npm run _build:dev -- serve",
    "_serve:prod": "npm run _build:prod -- serve",
    "build": "npm run _build:dev",
    "build:production": "npm run _build:prod",
    "build:ci:production": "npm run _build:ci:prod",
    "serve": "npm run _serve:dev",
    "serve:production": "npm run _serve:prod",
    "clean": "rm -Rf public/* resources"
  },
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "eslint": "9.2.0",
    "eslint-plugin-jsdoc": "48.2.4",
    "globals": "15.2.0",
    "hugo-extended": "0.124.1",
    "markdownlint": "0.34.0",
    "markdownlint-cli": "0.40.0",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-go-template": "0.0.15",
    "stylelint": "16.5.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "6.3.0"
  }
}