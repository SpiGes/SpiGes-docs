{
    "name": "spiges-handbook",
    "version": "0.0.1",
    "description": "spiges-handbook web site",
    "repository": "github:spiges/handbook",
    "homepage": "https://spiges.github.io/handbook",
    "author": "SpiGes team",
    "license": "CC BY-NC-ND 4.0",
    "spelling": "cSpell:ignore HTMLTEST precheck postbuild -",
    "scripts": {
        "_env:prod": "npx cross-env NODE_ENV=production HUGO_ENV=production HUGO_ENVIRONMENT=production",
        "_env:dev": "npx cross-env NODE_ENV=development HUGO_ENV=development HUGO_ENVIRONMENT=development",
        "_hugo": "hugo --logLevel debug --gc --cleanDestinationDir",
        "_build:dev": "npm run _hugo -- -e development -DFE",
        "_build:prod": "npm run _hugo -- -e production --minify",
        "build": "npm run _env:dev -- npm run _build:dev",
        "build:production": "npm run _env:prod -- npm run _build:prod",
        "build:ci:production": "npm run _env:prod -- npm run _build:prod -- --baseURL \"${BASE_URL}\"",
        "serve": "npm run _env:dev -- npm run _build:dev -- serve",
        "serve:production": "npm run _env:prod -- npm run _build:prod -- serve",
        "clean": "rm -Rf public/* resources"
    },
    "devDependencies": {
        "@eslint/js": "9.14.0",
        "autoprefixer": "10.4.20",
        "cross-env": "7.0.3",
        "eslint": "9.14.0",
        "eslint-plugin-jsdoc": "50.5.0",
        "globals": "15.12.0",
        "hugo-extended": "0.133.1",
        "markdownlint": "0.36.1",
        "markdownlint-cli": "0.42.0",
        "postcss": "8.4.49",
        "postcss-cli": "11.0.0",
        "prettier": "3.3.3",
        "prettier-plugin-go-template": "0.0.15",
        "stylelint": "16.7.0",
        "stylelint-config-standard-scss": "13.1.0",
        "stylelint-scss": "6.4.1"
    }
}